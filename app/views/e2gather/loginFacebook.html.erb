<ul>
<div><%= link_to "Logout\n", "/logout" %></div>
 

<%= "Welcome to E2gather: " +@current_user["name"] %> (<i><%=@current_user["user_id"] %></i>)
 
 
 
<%= form_tag("ingre", method: "post") do %>
    <div><%= text_field_tag :my_input %></div>
    <div><%= submit_tag "Find E2gathers"%></div>
<% end %>

<%= form_tag("render_event_page", method: "get") do %>
    <div><%= submit_tag "Create New Event" %></div>
	
<% end %>
 
<div><%=" Your E2Gather Friends:"%></div>
<% @friend_list.each do |f| %>
<%= f["name"] %> (<i><%=f["id"] %></i>)
<%= form_tag("sendmail", method: "post", ) do %>
    <%= text_field_tag :my_email %>
	<%=hidden_field_tag :name, f["name"] %>
	<%=hidden_field_tag :id, f["id"] %>
    <%= submit_tag "Send Email to " +f["name"] %>
<% end %>
<% end %>
 
 
 